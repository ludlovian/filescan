{
  "name": "filescan",
  "version": "1.1.1",
  "description": "recursive file scan as async generator",
  "main": "dist/index.js",
  "source": "src/index.js",
  "module": "dist/index.mjs",
  "scripts": {
    "clean": "rm -rf dist/ node_modules/.cache/ .nyc_output/ coverage/",
    "format": "prettier-standard src/*.js test/*.js",
    "lint": "standard src/*.js test/*.js | snazzy",
    "test": "nyc ava",
    "coverage": "npm run test && nyc report --reporter=html && sirv coverage/ -p 8000",
    "build": "rollup -c rollup.config.js"
  },
  "repository": {
    "type": "git",
    "url": "/nas/config/git/filescan.git"
  },
  "author": "",
  "license": "Unlicense",
  "ava": {
    "require": [
      "esm"
    ]
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "esm": "^3.2.22",
    "nyc": "^13.3.0",
    "prettier-standard": "^9.1.1",
    "rollup": "^1.10.0",
    "rollup-plugin-cleanup": "^3.1.1",
    "rollup-plugin-terser": "^4.0.4",
    "sinon": "^7.3.1",
    "sirv-cli": "^0.4.2",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1"
  }
}
